{
  "name": "react-redux-saga",
  "version": "0.1.0",
  "description": "Frontend application using The Movie Database API",
  "author": "Joan Roucoux",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest --passWithNoTests",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --cache-location ./.eslintcache \"./**/*.js\" \"./**/*.jsx\" --cache",
    "lint:fix": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --cache-location ./.eslintcache \"./**/*.js\" \"./**/*.jsx\" --cache --fix",
    "jest:clear-cache": "jest --clearCache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "debug": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.4.1",
    "react-slick": "^0.26.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "chai": "^4.2.0",
    "customize-cra": "^0.9.1",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.5",
    "jest-image-snapshot": "^3.1.0",
    "react-app-rewired": "^2.1.6",
    "redux-mock-store": "^1.5.4",
    "redux-saga-test-plan": "^4.0.0-rc.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
